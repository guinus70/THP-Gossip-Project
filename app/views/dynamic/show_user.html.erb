<div class="card text-center">
<div class="card-header">
  Profile
</div>
<div class="card-body">
  <h5 class="card-title"><%= params[:user].first_name %> <%= params[:user].last_name %></h5>
  <p class="card-text"><%= params[:user].age %> ans</p>
  <p class="card-text"><%= params[:user].description %></p>
</div>
<div class="card-footer text-muted">
<%= params[:user].city.name %>
</div>
</div>
